<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<!-- Configures the handler for the ASP.NET Core application -->
		<handlers>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
		</handlers>

		<!-- Specific configuration for ASP.NET Core module -->
		<aspNetCore processPath="dotnet" arguments=".\BlogNotes.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="InProcess" />
	</system.webServer>
</configuration>
